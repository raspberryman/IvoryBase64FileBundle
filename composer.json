{
    "name": "egeloen/base64-file-bundle",
    "type": "symfony-bundle",
    "description": "Provides a base64 file support for your Symfony2 Project.",
    "keywords": [ "base64", "file" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "symfony/framework-bundle": "^2.7|^3.0",
        "symfony/form": "^2.7|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^2.7|^3.0",
        "symfony/validator": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\Base64FileBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
